{
        "version": "1.0",
        "build":[
                {
                        "src":"build.sh",
                        "use": "@vercel/static-build",
                        "config":{
                                "distDir":"public"
                        }
                }
        ],
        "routes": [
                {
                        "src": "/(.*)",
                        "dest": "/public/$1"
                }
        ]
}